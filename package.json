{
  "name": "file-upload-api",
  "version": "0.1.0",
  "private": true,
  "main": "src/index.ts",
  "scripts": {
    "start": "ts-node src/index.ts",
    "build": "tsc",
    "lint": "eslint . --fix",
    "test": "cross-env NODE_ENV=test jest --runInBand",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "dev": "cross-env NODE_ENV=development ts-node-dev --respawn src/index.ts",
    "format": "prettier --write ."
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "verbose": false,
    "testRegex": "/tests/.*\\.ts"
  },
  "dependencies": {
    "@sendgrid/mail": "^7.2.1",
    "@types/bcrypt": "^3.0.0",
    "@types/cheerio": "^0.22.21",
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.7",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.157",
    "@types/node": "^14.14.2",
    "@types/node-fetch": "^2.5.7",
    "aws-sdk": "^2.803.0",
    "bcrypt": "^5.0.0",
    "cheerio": "^1.0.0-rc.3",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.0",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.10",
    "shortid": "^2.2.15",
    "socket.io": "^3.0.3",
    "ts-node": "^8.10.2",
    "typeorm": "^0.2.29",
    "typescript": "^3.9.5"
  },
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "jest": "^26.6.3",
    "prettier": "2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0-pre.49"
  }
}
